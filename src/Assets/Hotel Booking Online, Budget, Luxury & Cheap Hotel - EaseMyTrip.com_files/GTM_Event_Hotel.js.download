function hotelViewEvent(city,name,hid,cin,cout,totalguest,totalroom,webType,coupon,price,tax)
{ try {
	dataLayer.push({
  event: "view_item",
  ecommerce: {
   
      value: parseInt(price),
      tax: parseInt(tax),
      currency: "INR",
      coupon: coupon,
      items: [
       {
        item_id: hid,
        item_name: name,
        coupon: coupon,
        item_brand: "Hotel",
        item_category: city,
        item_category2: cin,
        item_category3: cout,
        item_category4: totalguest,
        item_category5: totalroom,
item_list_id: "Hotel",
item_list_name: "Hotel",
        item_variant: webType,
        price: parseInt(price) + parseInt(tax),
        quantity: 1,
      }
]
	}})
}
    catch (e) { }

	
}

