function ViewFBListing(checkin, checkout, listId, city, country, num_adlts, num_child) {
    LoadFBPiixel();
    try {
        fbq('track', 'Search', {
            content_type: 'hotel',
            checkin_date: checkin,
            checkout_date: checkout,
            content_ids: '' + listId + '',
            city: city,
          
            country: country,
            num_adults: parseInt(num_adlts),
            num_children: parseInt(num_child)
        });
    }
    catch (e) {
    }
}

